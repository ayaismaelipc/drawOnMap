initializeMapEditor(mapInstance, options)
options = {
    onCreateCallback?,
    onEditCallback?,
    onDeleteCallback?
}

startDrawing(type, sourceId)
type = point / polygon / circle/ line
startEditing(sourceId)
startDeleting(sourceId)

